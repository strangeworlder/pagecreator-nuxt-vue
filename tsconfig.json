{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "moduleDetection": "force",
    "resolveJsonModule": true,
    "allowJs": true,
    "types": ["@types/node", "nuxt"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules", ".nuxt", "dist", ".output"]
}
